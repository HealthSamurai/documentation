    "lvl0": {
      "selector": "article nav li:last-child",
      "global": true,
      "default_value": "Blog"
    },
    "lvl1": {
      "selector": "article h1",
      "global": true,
      "default_value": "Article"
    },
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5",
    "lvl6": "article h2 ~ table tr:nth-child(4) td, article h2 ~ table tr:nth-child(5) td, article h3 ~ table tr:nth-child(4) td, article h3 ~ table tr:nth-child(5) td",
    "text": "article.prose p, article.prose li, article.prose td, article.prose blockquote, article.prose pre, article.prose code",
    "tags": "#doc-tags span"
  },
  "custom_settings": {
    "filterableAttributes": [
      "url",
      "hierarchy_lvl1",
      "hierarchy_lvl2",
