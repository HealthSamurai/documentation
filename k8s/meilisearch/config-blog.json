      "hierarchy_lvl5",
      "hierarchy_lvl6",
      "tags"
    ],
    "displayedAttributes": [
      "hierarchy_lvl1",
      "hierarchy_lvl2",
      "hierarchy_lvl3",
      "hierarchy_lvl4",
      "hierarchy_lvl5",
      "hierarchy_lvl6",
      "content",
      "anchor",
      "objectID",
      "url",
      "hierarchy_lvl0",
      "tags"
    ],
    "searchableAttributes": [
      "hierarchy_lvl1",
      "hierarchy_lvl2",
      "hierarchy_lvl3",
      "hierarchy_lvl4",
      "hierarchy_lvl5",
      "hierarchy_lvl6",
      "content",
      "hierarchy_lvl0",
      "tags"
    ],
    "rankingRules": [
      "attribute",
      "words",
      "typo",
      "proximity",
      "sort",
      "exactness"
    ]
  }
